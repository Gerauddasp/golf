/* From FOGT CSS */
/* well */
.well {
 min-height: 20px;
 padding: 30px /*12px*/3%;
 margin: 20px 0;
 clear: right;
 background-color: /*rgba(242,241,234,0.10)*/#fff;
 border: 1px solid #eee;
 border: 1px solid rgba(0, 0, 0, 0.05);  box-shadow: 0 5px 5px 0 rgba(0,0,0,0.24),0 5px 5px 0 rgba(0,0,0,0.19);
 /*box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19);*/
}
.well_reduced {  min-height: 20px;
 padding: 19px;
 margin: 1% 1% 2% 2%;
