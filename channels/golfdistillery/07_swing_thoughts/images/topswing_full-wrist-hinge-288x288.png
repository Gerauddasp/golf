.clear {clear: both;}
/* To hide recaptcha badge while keeping it activated */
.grecaptcha-badge {
visibility: hidden !important;}
img.alignright {margin-bottom: 1em;}
hr {clear: both;}
/* Mediavine Requirement to push sidebar to the bottom  */
@media (max-width: 1023px) {
 #primary, #right-sidebar {
 width: 100% !important;
 float: none !important;;
 }
}
/* To Shorten space between two H1 in category pages */
.category-h1 {margin-top: -0.75em; text-align: center;}
h1 {text-align: center;}
h2 {clear: right;}
h3 {clear: right;}
.boxdds {text-align: center; width: 160px; height: 200px; -moz-hyphens: none;}
.bottomborderdds {border-bottom: 1px solid rgba(0,0,0,.1); margin-bottom: 5px; padding-bottom: 10px;}
.wp-caption.alignleft {margin: 1.5% 1.2% 1.5% 0;}
.cat-links {font-size: 1.4em;}
.load-more {text-align: center; margin: 25px 25px;}
/* To center elements of the 2nd footer (site navigation) */
.footer-widget-1, .footer-widget-2, .footer-widget-3 {text-align: center;}
/* To Allow Scrolling in Long Menu with Sticky Mobile Menu */
.main-navigation.toggled .main-nav>ul {
 max-height: calc(100vh - 60px);
 overflow-y: scroll;
}
/* To Display Full YouTube Video Thumbnails in Mobile, from FOGT */
.video{clear: right; margin-top: 20px; margin-bottom: 20px; position: relative; padding-bottom: 56.19%; background-color: #2f4e6f; height: 0; overflow: hidden; box-shadow: 0 5px 5px 0 rgba(47,78,111,0.1),0 5px 5px 0 rgba(47,78,111,0.5)}
.video iframe,
.video object,
.video embed {position: absolute; top: 0; left: 0; width: 100%; height: 100%;}
/* Shade Box around images and pages menu box */
a img:hover {box-shadow: 0 5px 10px 0 rgba(47,78,111,0.1),0 5px 10px 0 rgba(47,78,111,0.5);}
a img.header-image:hover {box-shadow: none;}
#subpages-widget-2 {box-shadow: 0 5px 10px 0 rgba(47,78,111,0.1),0 5px 10px 0 rgba(47,78,111,0.5); margin: 25px 10px;}
/* Bottom of Page Image Links */ 
.bottomLinks {text-align: center;}
.bottomLinks a {display: inline-block; margin-bottom: 0.75em; margin-left: 0.5em; margin-right: 0.5em;}
.Selected {opacity: 0.25;}
.entry-content ol li { 
 margin-bottom: 1.2em;
}
/* Sub Pages Styles */
.widget_subpages .widget-title { margin: 0 0 0 0; padding: 0; display: block; background: #F5F5F5; border: 1px solid #E1E1E8; border-bottom: none; font-weight: normal; font-size: 0.95em;}
.widget_subpages .widget-title a {padding: 8px 15px; margin: 0; display: block; background: #2f4e6f; color: #ffffff;}
.widget_subpages .widget-title a:hover { background: #5d5d5d; text-decoration: none;}
.widget_subpages ul { padding: 0; margin: 0; border-top: 1px solid #E1E1E8;}
.widget_subpages ul ul { border-top: none;}
.widget_subpages li { list-style-type: none; padding: 0; margin: 0; }
/* Level 1*/
.widget_subpages li a { padding: 4px; display: block; background: #F5F5F5; border: 1px solid #E1E1E1; border-top: none; font-size: 0.95em;}
.widget_subpages li a:hover { background: #ddd; text-decoration: none;}
.widget_subpages li.widget_subpages_current_page a { color: #fff; background-color: #5d5d5d;}
/* Level 2 */
.widget_subpages ul li a { padding-left: 18px; background: rgba(47, 78, 111, 0.05); font-size: 0.95em;}
.widget_subpages ul li a:hover { background: #ddd; text-decoration: none; }
.widget_subpages ul li.widget_subpages_current_page a { color: #fff; background: #556e89;}
/* Level 3 */
.widget_subpages ul ul li a { color: inherit; padding-left: 36px; background: #e7e7e7; font-size: 0.95em; }
.widget_subpages ul ul li a:hover { background: #ddd; }
.widget_subpages ul ul li.widget_subpages_current_page a { color: #fff; background: #556e89;}
/* Level 4 */
.widget_subpages ul ul ul li a { color: inherit; padding-left: 54px; background: #e7e7e7;  font-size: 0.95em;}
.widget_subpages ul ul ul li a:hover { background: #ddd; }
.widget_subpages ul ul ul li.widget_subpages_current_page a { color: #fff; background: #556e89;}
.widget_subpages h1 { font-size: 1.0em; }
.widget_subpages ul li {margin: 0; padding: 0; border: none;}
/* Fix to account for fixed menu when using anchor tags */
:target:before {
content:"";
display:block;
height:110px; /* fixed header height*/
margin:-110px 0 0; /* negative fixed header height */
}
/* Fix for padding in pages in section widget */
div:not(.entry-content) .widget ul li {padding-bottom:0;}
/* Pagination */
.page-numbers {
 padding: 5px;
 background-color: #5d5d5d;
 border-radius: 0px;
 color: #fff !important;
 border: 5px solid #556e89; 
 margin-right: 0.25em;
 font-size: 1.2em;
 text-align: center;
 height: 50px;
}
.page-numbers:hover,
.page-numbers.current {
 background-color: #fff;
